{
  "name": "lattice",
  "version": "1.0.0",
  "description": "Lattice coding assignment",
  "main": "index.js",
  "scripts": {
    "copy": "mkdir -p build/$NODE_ENV && cp -r src/index.html build/$NODE_ENV",
    "copy:dev": "NODE_ENV=development npm run copy",
    "lint": "eslint --ext .js --ext .jsx src",
    "serve": "NODE_ENV=development node ./server/index.js",
    "start": "npm run copy:dev && NODE_ENV=development webpack-dev-server --progress --colors --hot --config ./webpack.js --mode development",
    "watch": "NODE_ENV=development nodemon --exec npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mwnachman/lattice.git"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@koa/cors": "^3.0.0",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.4",
    "css-loader": "^0.28.7",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^8.0.0",
    "node": "^12.16.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^5.1.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.16.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.12.3",
    "mini-css-extract-plugin": "^0.4.4",
    "nodemon": "^1.12.5",
    "pre-commit": "^1.1.2",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.1.9"
  },
  "pre-commit": [
    "lint"
  ]
}
